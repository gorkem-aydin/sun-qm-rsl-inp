<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Dialog id="dialogOutOfTolerance" stretch="{device>/system/phone}" contentWidth="10%" busyIndicatorDelay="0">
		<content>
			<form:SimpleForm id="simpleFormOutOfTolerance" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4"
				labelSpanS="4" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="1">
				<form:content>
					<!--<l:BlockLayoutRow>-->
					<!--	<l:BlockLayoutCell>-->
					<Label/>
					<Button id="buttonDialogOOTTestAgain" text="{i18n>testAgain}" press="onDialogOOTTestAgainPress"/>
					<Label/>
					<Button id="buttonDialogOOTApprove" text="{i18n>approve}" press="onDialogOOTApprovePress"/>
					<Label/>
					<Button id="buttonDialogOOTExcludeData" text="{i18n>excludeData}" press="onDialogOOTExcludeDataPress"/>
					<!--	</l:BlockLayoutRow>-->
					<!--</l:BlockLayoutCell>-->
				</form:content>
			</form:SimpleForm>
		</content>
		<buttons>
			<Button id="buttonDialogOOTCancel" icon="sap-icon://sys-cancel" text="{i18n>cancel}" press="onDialogOOTButtonCancelPress"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>